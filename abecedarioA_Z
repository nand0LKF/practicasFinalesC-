#include <iostream>
#include <algorithm>  
using namespace std;

char caracter[] = {'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z'};

int numero[26];  
char c[26];      

char generarCaracte(int index)//index representa un vector de coordenadas de N enteros que especifica una posiciÃ³n Ãºnica en un espacio de N dimensiones
{
	char generarCaracte;
	do
	{
		generarCaracte = caracter[rand() % 26];
	} while (std::count(c, c + index, generarCaracte) > 0);  
	return generarCaracte;
}

int generarNumero()
{
	return rand() % 51;
}

void imprimir()
{
	cout << "CAR\tNUM" << endl;
	for (int i = 0; i < 26; i++)  
		cout << c[i] << "\t" << numero[i] << endl;
}

void generarDatos()
{
	for (int i = 0; i < 26; i++)  
	{
		numero[i] = generarNumero();
		c[i] = generarCaracte(i);
	}
}

int main()
{
	generarDatos(); 
	imprimir();
	return 0;
}
